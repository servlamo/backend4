<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2010 rel. 3 (http://www.altova.com) by MESMERiZE (MSM) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:altova="http://www.altova.com/xml-schema-extensions" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Version 1.0: -->
	<xs:element name="AccrdtnCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>Код аккредитации</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AccrdtnName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Название аккредитации</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AcctBal">
		<xs:annotation>
			<xs:documentation>Остаток счета</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="BalType" type="xs:string">
					<xs:annotation>
						<xs:documentation>Тип остатка
AcctBalance - текущий остаток средств на счёте
CalcPrc - сумма расчитанных, но не выплаченных %%
AcctAvailableBal - доступный остаток на счете
AcctMinBalance - наличие минимального остатка по  счету
OpeningLedger - остаток на начало периода
ClosingLedger - остаток на конец периода</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="AcctBalance"/>
								<altova:example value="AcctAvailableBal"/>
								<altova:example value="CalcPrc"/>
								<altova:example value="AcctMinBalance"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:element name="CurAmt" type="CurrencyAmount">
					<xs:annotation>
						<xs:documentation>Величина остатка</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EffDt" type="DateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата остатка</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ExpDt" type="DateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата окончания для остатка</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Desc" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="AcctCur" type="xs:string">
		<xs:annotation>
			<xs:documentation>Валюта счета</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AcctId">
		<xs:annotation>
			<xs:documentation>Номер лицевого счета</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="20"/>
				<xs:pattern value="[0-9]+"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AcctType" type="xs:string">
		<xs:annotation>
			<xs:documentation>Тип счета:
DDA – текущий счет
SDA – сберегательный счет
TDA – срочный депозит
CDA – депозитный сертификат
MMA – счет денежного рынка
CMA – счет управления наличностью</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="DDA"/>
					<altova:example value="SDA"/>
					<altova:example value="TDA"/>
					<altova:example value="CDA"/>
					<altova:example value="MMA"/>
					<altova:example value="CMA"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="ActCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>Номер акта</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Active" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>Признак активности</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AddInfo">
		<xs:annotation>
			<xs:documentation>Дополнительная информация</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Parm" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AdditionalStatus">
		<xs:annotation>
			<xs:documentation>Дополнительные статусы</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="StatusCode"/>
				<xs:element ref="Severity" minOccurs="0"/>
				<xs:element ref="StatusDesc" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgreemtInfo">
		<xs:annotation>
			<xs:documentation>Информация о договоре</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="AgreemtNum"/>
				<xs:element name="AgreemtDt" type="Date">
					<xs:annotation>
						<xs:documentation>Дата договора</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CurAmt" type="CurrencyAmount" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сумма валюты договора</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="AgreemtNum" type="xs:string">
		<xs:annotation>
			<xs:documentation>Номер договора</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BankAcctInfo" type="BankAcct_Type">
		<xs:annotation>
			<xs:documentation>Информация о счете</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BankAcctList">
		<xs:annotation>
			<xs:documentation>Список счетов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="BankAcctRec" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BankAcctRec" type="BankAcct_Type">
		<xs:annotation>
			<xs:documentation>Запись о счете</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BankInfo">
		<xs:annotation>
			<xs:documentation>Информация о Банке</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="BankIdType" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип идентификатора банка.
Предопределенные значения:
SWIFT – BIC S.W.I.F.T.
RUS.BIC – БИК, присваиваемый ЦБ РФ
</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BankId" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>БИК</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Name" minOccurs="0"/>
				<xs:element ref="TaxId" minOccurs="0"/>
				<xs:element ref="TaxRegCode" minOccurs="0"/>
				<xs:element ref="RUS.CorrAcct" minOccurs="0"/>
				<xs:element name="BranchId" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Идентификатор филиала</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BranchName" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Наименование филиала</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AgencyId" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код подразделения  счета </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AgencyName" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Наименование отделения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PostAddr" type="Addr_Type" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="BankSvcRq">
		<xs:annotation>
			<xs:documentation>Данные запроса</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all/>
		</xs:complexType>
	</xs:element>
	<xs:element name="BankSvcRs">
		<xs:annotation>
			<xs:documentation>Данные ответа</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="Status"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Birthday" type="Date">
		<xs:annotation>
			<xs:documentation>Дата рождения</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BirthPlace" type="xs:string">
		<xs:annotation>
			<xs:documentation>Место рождения</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BpId" type="xs:string" default="BP_ID">
		<xs:annotation>
			<xs:documentation>Идентификатор БП в РБО</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardActualStatus">
		<xs:annotation>
			<xs:documentation>Информация об актуальном статусе карты</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="CardStatusCode"/>
				<xs:element ref="StatusDesc" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CardBarCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>Штрих-код карты</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardId" type="xs:string">
		<xs:annotation>
			<xs:documentation>Идентификатор карты в РБО</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardInfo" type="CardInfo_Type">
		<xs:annotation>
			<xs:documentation>Данные карты</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardKindCode">
		<xs:annotation>
			<xs:documentation>Тип пластика</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardKindName">
		<xs:annotation>
			<xs:documentation>Платежная система</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardList">
		<xs:annotation>
			<xs:documentation>Список карт</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CardRec" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CardRec" type="CardInfo_Type">
		<xs:annotation>
			<xs:documentation>Запись о карте из списка</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CardStatus">
		<xs:annotation>
			<xs:documentation>Статус пластиковой карты</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="CardStatusCode"/>
				<xs:element ref="StatusDesc" minOccurs="0"/>
				<xs:element name="EffDt" type="DateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата ввода в действие</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="StatusModReason" minOccurs="0"/>
				<xs:element ref="StatusModBy" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CardStatusCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>Код статуса пластиковой карты</xs:documentation>
			<xs:appinfo>
				<altova:exampleValues>
					<altova:example value="WRK"/>
				</altova:exampleValues>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="Category" type="xs:string">
		<xs:annotation>
			<xs:documentation>Категория</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Citizenship">
		<xs:annotation>
			<xs:documentation>Информация о гражданстве</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="Country"/>
				<xs:element ref="CountryName" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="ClassifierId">
		<xs:annotation>
			<xs:documentation>Организационно-правовая форма</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="ClassifierType" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип классификатора. Содержит символы из набора Latin-1.
Предопределенные значения:
RUS.OKPO – ОКПО
RUS.OKATO – ОКАТО
RUS.OKOPF – ОКОПФ
RUS.OKFS – ОКФС
RUS.SOOGU – СООГУ
RUS.BC – бюджетная классификация (классификация доходов бюджетов)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ClassifierCode" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код классификатора</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ClassifierCodeDetail" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код классификатора (иное)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CodeDate" type="xs:string">
		<xs:annotation>
			<xs:documentation>Кодовая дата</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Collateral">
		<xs:annotation>
			<xs:documentation>Обеспечение</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CollateralType" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип обеспечения. Предопределенные значения:
Guaranty – гарантия или поручительство
Mort – залог имущества
Secs – залог ценных бумаг
</xs:documentation>
						<xs:appinfo>
							<altova:exampleValues>
								<altova:example value="Guaranty"/>
								<altova:example value="Mort"/>
								<altova:example value="Secs"/>
							</altova:exampleValues>
						</xs:appinfo>
					</xs:annotation>
				</xs:element>
				<xs:choice>
					<xs:element ref="PersonInfo" minOccurs="0"/>
					<xs:element ref="OrgInfo" minOccurs="0"/>
					<xs:element name="CollateralDesc" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Описание обеспечения</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:element name="CurAmt" type="CurrencyAmount"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactInfo">
		<xs:annotation>
			<xs:documentation>Контактные данные клиента</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="ContactPref" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Предпочтения по контактам.
Предопределенные значения:
AlwPhone – звонить и днем, и вечером
DayPhone – звонить днем
EvePhone – звонить вечером
AlwFax – факс и днем, и вечером
DayFax – факс днем
EveFax – факс вечером
Mobile – мобильный телефон 
Email – e-mail
ccMail – Lotus cc:Mail
Post – почта
Telex – телекс</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="AlwPhone"/>
							<xs:enumeration value="DayPhone"/>
							<xs:enumeration value="EvePhone"/>
							<xs:enumeration value="AlwFax"/>
							<xs:enumeration value="DayFax"/>
							<xs:enumeration value="EveFax"/>
							<xs:enumeration value="Mobile"/>
							<xs:enumeration value="Email"/>
							<xs:enumeration value="ccMail"/>
							<xs:enumeration value="Post"/>
							<xs:enumeration value="Telex"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="PhoneNums" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="PhoneNum" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="EmailAddrs" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="EmailAddr" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PostAddrs" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PostAddr" type="Addr_Type" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="Country" type="xs:string">
		<xs:annotation>
			<xs:documentation>Код страны</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CountryName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Наименование страны</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Cust2CustList">
		<xs:annotation>
			<xs:documentation>Список взаимосвязей потребителей</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Cust2CustRec" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Cust2CustRec" type="Cust2CustInfo_Type">
		<xs:annotation>
			<xs:documentation>Запись о взаимосвязи потребителя</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustCategory">
		<xs:annotation>
			<xs:documentation>Категория клиента</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="CustCategoryCode"/>
				<xs:element ref="CustCategoryName" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustCategoryCode">
		<xs:annotation>
			<xs:documentation>Категория клиента (код)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Walk-IN"/>
				<xs:enumeration value="Corporate"/>
				<xs:enumeration value="AFK"/>
				<xs:enumeration value="Loyalty"/>
				<xs:enumeration value="SOTRUDNIK"/>
				<xs:enumeration value="SUPER_VIP"/>
				<xs:enumeration value="VIP_COMMON"/>
				<xs:enumeration value="VIP_CORP"/>
				<xs:enumeration value="VIP_DPMR"/>
				<xs:enumeration value="VIP_FIL"/>
				<xs:enumeration value="VIP_PERS"/>
				<xs:enumeration value="VIP_PERSONAL"/>
				<xs:enumeration value="VIP_STR"/>
				<xs:enumeration value="CATEGORY.WALK_IN"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CustCategoryName" type="xs:string">
		<xs:annotation>
			<xs:documentation>Наименование категории</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustId" type="xs:string">
		<xs:annotation>
			<xs:documentation>Идентификатор клиента РБО</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustInfo" type="CustInfo_Type">
		<xs:annotation>
			<xs:documentation>Данные клиента</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustList">
		<xs:annotation>
			<xs:documentation>Список клиентов</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CustRec" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustRec" type="CustInfo_Type">
		<xs:annotation>
			<xs:documentation>Запись о клиенте из списка</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustResponsibleInfo">
		<xs:annotation>
			<xs:documentation>Информация об ответственном лице потребителя</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="FullName" minOccurs="0"/>
				<xs:element ref="PersonName" minOccurs="0"/>
				<xs:element ref="ContactInfo" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="CustStatus">
		<xs:annotation>
			<xs:documentation>Статус потребителя</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element ref="CustStatusCode"/>
				<xs:element ref="StatusDesc" minOccurs="0"/>
				<xs:element name="EffDt" type="DateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата/время ввода в действие</xs:documentation>
