	<xs:complexType name="NHResultBlock">
		<xs:annotation>
			<xs:documentation>Тип данных, описывающий выходной вектор NH</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MatchSummary">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TotalMatchScore" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Общий скоринговый балл</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:union memberTypes="xs:int emptyType"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Rules" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сработавшие правила</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Rule" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="RuleID">
													<xs:annotation>
														<xs:documentation>Правило</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Score" type="xs:integer">
													<xs:annotation>
														<xs:documentation>Скоринговый балл правила</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="ruleCount" use="required">
												<xs:annotation>
													<xs:documentation>Количество срабатываний</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:int">
														<xs:minExclusive value="0"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="isGlobal">
												<xs:simpleType>
													<xs:restriction base="xs:int">
														<xs:minInclusive value="0"/>
														<xs:maxInclusive value="1"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="totalRuleCount" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:int">
											<xs:minExclusive value="0"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="MatchSchemes" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Совпадения в схемах сравнения</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Scheme" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="SchemeID">
													<xs:annotation>
														<xs:documentation>Номер сработавшей схемы</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:int">
															<xs:minExclusive value="0"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Score" type="xs:integer">
													<xs:annotation>
														<xs:documentation>Суммарный скоринговый балл по схеме</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="schemeCount" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:int">
											<xs:minExclusive value="0"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="matches" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ErrorWarnings">
				<xs:annotation>
					<xs:documentation>Служебная информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Errors">
							<xs:annotation>
								<xs:documentation>Ошибки</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Error" type="NHErrorWarningType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
								<xs:attribute name="errorCount" type="xs:int" use="required">
									<xs:annotation>
										<xs:documentation>Количество ошибок</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="Warnings">
							<xs:annotation>
								<xs:documentation>Предупреждения</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Warning" type="NHErrorWarningType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
								<xs:attribute name="warningCount" type="xs:int" use="required">
									<xs:annotation>
										<xs:documentation>Количество предупреждений</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="NHErrorWarningType">
		<xs:sequence>
			<xs:element name="Number" type="xs:int">
				<xs:annotation>
					<xs:documentation>Код</xs:documentation>
